  "bugs": {
    "url": "https://github.com/badri-s2001/antigravity-claude-proxy/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.12.14",
